{
  "name": "twitch-live-extension",
  "version": "4.3.2",
  "description": "A browser extension that will help you to find out when your favorite streamers are live on Twitch",
  "main": "index.js",
  "scripts": {
    "start": "node esbuild.dev.mjs",
    "build": "node esbuild.mjs",
    "build:chrome": "yarn run build && BROWSER=chrome node scripts/manifest/generateManifest.mjs",
    "build:firefox": "yarn run build && BROWSER=firefox node scripts/manifest/generateManifest.mjs",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "format": "yarn run lint --fix & yarn prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PedroS11/twitch-live-extension.git"
  },
  "author": "Pedro Silva",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/PedroS11/twitch-live-extension/issues"
  },
  "homepage": "https://github.com/PedroS11/twitch-live-extension#readme",
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.10",
    "@types/chrome": "^0.1.36",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "axios": "^1.13.2",
    "esbuild": "^0.27.2",
    "esbuild-css-modules-plugin": "^3.1.5",
    "esbuild-sass-plugin": "^3.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "live-server": "^1.2.2",
    "prettier": "^3.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.16.0",
    "typescript": "^5.9.3",
    "zustand": "^4.4.1"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
