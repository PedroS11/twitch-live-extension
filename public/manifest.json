{
  "name": "Twitch Live Extension Test",
  "description": "A browser extension that shows your favorites streamers that went live",
  "version": "2.0.0",
  "author": "Pedro Silva",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "Twitch Live",
    "default_popup": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2,
  "icons": {
    "16": "assets/logoTwitch.png",
    "48": "assets/logoTwitch.png",
    "128": "assets/logoTwitch.png"
  }
}
